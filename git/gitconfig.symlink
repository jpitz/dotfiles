[alias]
    st = status

[user]
    name = Mya Pitzeruse
    email = j.mya.pitz@gmail.com

[alias]
    co = checkout

[color]
    diff = auto
    status = auto
    branch = auto
    ui = true

[core]
    excludesfile = ~/.gitignore
    editor = vim
    autocrlf = input

[apply]
    whitespace = nowarn

[mergetool]
    keepBackup = false

[difftool]
    prompt = false

[help]
    autocorrect = 1

[push]
    default = simple

[credential]
    helper = osxkeychain
